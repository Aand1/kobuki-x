<launch>
  <arg name="frame_id"    default="laser_scan"/>

  <node pkg="urg_node" name="urg_node" type="urg_node"> 
    <param name="frame_id"  value="$(arg frame_id)"/>
    <param name="calibrate_time" type="bool" value="true"/>  
    <param name="intensity" type="bool" value="false"/>
    <param name="min_ang" value="-2.356194437"/>   
    <param name="max_ang" value="2.35619443"/>    
    <param name="cluster" value="1"/>
  </node>

  <node pkg="tf" type="static_transform_publisher" name="link_to_scan" args="-0.014 0 0.187 0 0 0 base_link $(arg frame_id) 100" />

</launch>
